{
    "version": 2,
    "routes": [
      {
        "src": "^/service-worker.js$",
        "dest": "/_next/static/service-worker.js",
        "headers": {
          "cache-control": "public, max-age=43200, immutable",
          "Service-Worker-Allowed": "/"
        }
      }
    ],
    "builds": [
      {
        "src": "next.config.js",
        "use": "@now/next"
      }
    ],
    "build": {
      "env": {
          "API_URL": "@staging-url",
          "GA_ID": "@ga_id"
      }
    },
    "env": {
        "API_URL": "@staging-url",
        "GA_ID": "@ga_id"
    }
  }